{
  "name": "PROJ",
  "version": "0.0.1",
  "description": "A website build by MMT Digital",
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:snapshots": "jest --updateSnapshot",
    "test:watch": "jest --watch -o",
    "build:dev": "node -r @babel/register node_modules/webpack/bin/webpack --config config/webpack/dev/webpack.config.dev.js --mode=development",
    "build:prod": "node -r @babel/register node_modules/webpack/bin/webpack --config config/webpack/prod/webpack.config.prod.js --mode=production",
    "prestart": "rimraf build",
    "start": "npm run build:dev",
    "lint:js": "standard \"src/**/*.js\"",
    "lint:js-fix": "standard --fix",
    "lint:styles": "stylelint \"src/**/*.scss\"",
    "prepush": "npm run lint:styles && npm run lint:js"
  },
  "repository": {
    "type": "git",
    "url": "git@bitbucket.org:mmtdigital/mmt-digital-webpack-structure.git"
  },
  "author": "MMT Digital",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-async-generator-functions": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-do-expressions": "^7.8.3",
    "@babel/plugin-proposal-export-default-from": "^7.8.3",
    "@babel/plugin-proposal-export-namespace-from": "^7.8.3",
    "@babel/plugin-proposal-function-bind": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.6",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-react-constant-elements": "^7.9.0",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/plugin-transform-regenerator": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/register": "^7.9.0",
    "@mmtdigital/stylelint-config": "^1.0.4",
    "@skatejs/bore": "^5.0.2",
    "@skatejs/val": "^0.5.0",
    "autoprefixer": "^9.7.6",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "babel-plugin-transform-remove-console": "^6.9.0",
    "cross-env": "^7.0.2",
    "css-loader": "^3.5.3",
    "cssnano": "^4.1.10",
    "del-webpack-plugin": "^1.0.3",
    "enzyme": "^3.11.0",
    "favicons-webpack-plugin": "3.0.1",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "html-loader": "^1.1.0",
    "husky": "^4.2.5",
    "identity-obj-proxy": "^3.0.0",
    "imagemin": "^7.0.1",
    "img-loader": "^3.0.1",
    "jest": "^26.1.0",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "postcss-scss": "^2.0.0",
    "rimraf": "^3.0.2",
    "sass-loader": "^9.0.2",
    "sass-resources-loader": "^2.0.3",
    "snazzy": "^8.0.0",
    "standard": "14.3.4",
    "standard-loader": "^7.0.0",
    "stylelint": "^13.3.3",
    "stylelint-webpack-plugin": "^2.1.0",
    "terser-webpack-plugin": "^3.0.0",
    "url-loader": "^4.1.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^5.0.9"
  },
  "standard": {
    "globals": [
      "jest",
      "describe",
      "it",
      "expect",
      "shallow",
      "snapshot",
      "test",
      "enzyme",
      "mount",
      "after",
      "afterEach",
      "beforeEach",
      "parent"
    ],
    "ignore": [
      "node_modules/**"
    ],
    "parser": "babel-eslint"
  },
  "stylelint": {
    "extends": "@mmtdigital/stylelint-config"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|html)$": "<rootDir>config/jest/jest.filemock.js",
      "\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./config/jest/jest.setup.js"
    ],
    "modulePaths": [
      "<rootDir>/src"
    ],
    "collectCoverageFrom": [
      "src/**/*.js"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    },
    "verbose": true
  },
  "dependencies": {
    "core-js": "^3.6.5",
    "domready": "^1.0.8",
    "regenerator-runtime": "^0.13.5"
  }
}
